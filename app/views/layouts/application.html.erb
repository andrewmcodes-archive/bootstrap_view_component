<!DOCTYPE html>
<html>
  <head>
    <title>Bootstrap View Component</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application", media: "all", 'data-turbolinks-track': "reload" %>
    <%= javascript_pack_tag "application", 'data-turbolinks-track': "reload" %>
  </head>
  <body>
    <%= render(ContainerComponent.new) do %>
      <%= yield %>
    <% end %>
  </body>
</html>
